  "source": "pragma solidity ^0.4.8;\n\nimport \"./TimelineDocument.sol\";\n\n/**\n * A TimelineDocument that has the ability to store a certifier address\n * indicating that it has been certified by accredited parties that the\n * document is accurate in it's intentions.\n *\n * Copyright (c) 2017 Cognate, Inc.\n */\ncontract CertifiableTimelineDocument is TimelineDocument {\n    address public certifier;\n\n    function updateCertifier() public {//onlyOwner {\n        certifier = msg.sender;\n    }\n}",
