  "source": "pragma solidity ^0.4.8;\n\nimport \"./TimelineDocument.sol\";\n\n/**\n * A TimelineDocument that requires proof to backup the claim\n * of the data and intention of that document. Proof is represented\n * by the addresses of other contracts that store the proof documentation.\n *\n * Copyright (c) 2017 Cognate, Inc.\n */\ncontract Claim is TimelineDocument {\n    using ITimelineDocumentLib for ITimelineDocumentLib.ClaimStorage;\n\n    ITimelineDocumentLib.ClaimStorage claim;\n\n    // calls ITimelineDocumentLib.addProof()\n    function addProof(address proof) public {\n        claim.addProof(proof);\n    }\n\n\n    // Getters\n    //--------\n    \n    function proofs() public constant returns (address[]) {\n        return claim._proofs;\n    }\n}",
